@{
    Layout = null;
}

<!DOCTYPE html>
<html>

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>INSPINIA | Static Tables</title>

    <link href="~/AdminThema/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/AdminThema/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="~/AdminThema/css/plugins/iCheck/custom.css" rel="stylesheet">
    <link href="~/AdminThema/css/animate.css" rel="stylesheet">
    <link href="~/AdminThema/css/style.css" rel="stylesheet">

</head>

<body>

    <div id="wrapper">
        @await Component.InvokeAsync("AdminSidebar")
        <div id="page-wrapper" class="gray-bg">
            @await Component.InvokeAsync("AdminNavbar")
            <div class="wrapper wrapper-content animated fadeInRight">
                @RenderBody();
            </div>
            @await Component.InvokeAsync("AdminFooter")
        </div>
    </div>



    <script src="~/AdminThema/js/jquery-3.1.1.min.js"></script>
    <script src="~/AdminThema/js/popper.min.js"></script>
    <script src="~/AdminThema/js/bootstrap.js"></script>
    <script src="~/AdminThema/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/AdminThema/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>

    <script src="~/AdminThema/js/plugins/peity/jquery.peity.min.js"></script>

    <script src="~/AdminThema/js/inspinia.js"></script>
    <script src="~/AdminThema/js/plugins/pace/pace.min.js"></script>

    <script src="~/AdminThema/js/plugins/iCheck/icheck.min.js"></script>

    <script src="~/AdminThema/js/demo/peity-demo.js"></script>

    <script>
        $(document).ready(function () {
            $('.i-checks').iCheck({
                checkboxClass: 'icheckbox_square-green',
                radioClass: 'iradio_square-green',
            });
        });
    </script>

    @await RenderSectionAsync("Scripts", required: false)

</body>

</html>

